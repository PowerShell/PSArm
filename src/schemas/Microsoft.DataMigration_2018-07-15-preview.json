      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "SQL",
            "MySQL",
            "PostgreSql",
            "MongoDb",
            "Unknown"
          ]
        }
      }
    },
    "targetPlatform": {
      "propertyParameters": {
        "value": {
          "type": "string",
