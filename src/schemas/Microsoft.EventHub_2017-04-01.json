      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "blobContainer": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "archiveNameFormat": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "skipEmptyArchives": {
      "propertyParameters": {
        "value": {
          "type": "bool",
          "default": false
        }
      }
    },
    "userMetadata": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    }
