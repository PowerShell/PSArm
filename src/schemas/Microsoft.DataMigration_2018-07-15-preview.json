      }
    },
    "services/projects/files": {
      "extension": {
        "$ref": "#/$keywords/extension"
      },
      "filePath": {
        "$ref": "#/$keywords/filePath"
      },
      "mediaType": {
        "$ref": "#/$keywords/mediaType"
      }
    }
