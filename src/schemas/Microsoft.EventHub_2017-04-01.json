        "value": {
          "type": "string"
        }
      }
    },
    "ignoreMissingVnetServiceEndpoint": {
      "propertyParameters": {
        "value": {
          "type": "bool",
          "default": false
        }
      }
    },
    "ipRules": {
      "body": {
        "ipMask": {
          "$ref": "#/$keywords/ipMask"
        },
        "action": {
          "$ref": "#/$keywords/action"
        }
      },
      "array": true
    },
    "ipMask": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "action": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Allow"
          ]
        }
      }
    },
    "partnerNamespace": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "alternateName": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "messageRetentionInDays": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "partitionCount": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "status": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Active",
            "Disabled",
            "Restoring",
            "SendDisabled",
            "ReceiveDisabled",
            "Creating",
            "Deleting",
            "Renaming",
            "Unknown"
          ]
        }
      }
    },
