{
  "$keywords": {
    "registeredTools": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "ServerDiscovery",
            "ServerAssessment",
            "ServerMigration",
            "Cloudamize",
            "Turbonomic",
            "Zerto",
            "CorentTech",
            "ServerAssessmentV1",
            "ServerMigration_Replication",
            "Carbonite",
            "DataMigrationAssistant",
            "DatabaseMigrationService"
          ]
        }
      },
      "array": true
    },
    "provisioningState": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Accepted",
            "Creating",
            "Deleting",
            "Failed",
            "Moving",
            "Succeeded"
          ]
        }
      }
    },
    "tool": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "ServerDiscovery",
            "ServerAssessment",
            "ServerMigration",
            "Cloudamize",
            "Turbonomic",
            "Zerto",
            "CorentTech",
            "ServerAssessmentV1",
            "ServerMigration_Replication",
            "Carbonite",
            "DataMigrationAssistant",
            "DatabaseMigrationService"
          ]
        }
      }
    },
    "purpose": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Discovery",
            "Assessment",
            "Migration"
          ]
        }
      }
    },
    "goal": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Servers",
            "Databases"
          ]
        }
      }
    },
    "status": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Inactive",
            "Active"
          ]
        }
      }
    },
    "cleanupState": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "None",
            "Started",
            "InProgress",
            "Completed",
            "Failed"
          ]
        }
      }
    },
    "summary": {
      "body": {}
    },
    "details": {
      "body": {
        "groupCount": {
          "$ref": "#/$keywords/groupCount"
        },
        "assessmentCount": {
          "$ref": "#/$keywords/assessmentCount"
        },
        "extendedDetails": {
          "$ref": "#/$keywords/extendedDetails"
        }
      }
    },
    "groupCount": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "assessmentCount": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "extendedDetails": {
      "body": {}
    }
  },
  "$resources": {
    "migrateProjects": {
      "registeredTools": {
        "$ref": "#/$keywords/registeredTools"
      },
      "provisioningState": {
        "$ref": "#/$keywords/provisioningState"
      }
    },
    "migrateProjects/solutions": {
      "tool": {
        "$ref": "#/$keywords/tool"
      },
      "purpose": {
        "$ref": "#/$keywords/purpose"
      },
      "goal": {
        "$ref": "#/$keywords/goal"
      },
      "status": {
        "$ref": "#/$keywords/status"
      },
      "cleanupState": {
        "$ref": "#/$keywords/cleanupState"
      },
      "summary": {
        "$ref": "#/$keywords/summary"
      },
      "details": {
        "$ref": "#/$keywords/details"
      }
    }
  }
}
