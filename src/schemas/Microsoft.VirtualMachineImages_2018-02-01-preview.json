{
    "$keywords": {
        "source": {
            "parameters": {
                "type": {
                    "type": "string"
                }
            }
        },
        "customize": {
            "parameters": {
                "type": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                }
            },
            "array": true
        },
        "distribute": {
            "parameters": {
                "type": {
                    "type": "string"
                },
                "runOutputName": {
                    "type": "string",
                    "pattern": "^[A-Za-z0-9-_]{1,64}$"
                },
                "artifactTags": {}
            },
            "array": true
        },
        "provisioningState": {
            "propertyParameters": {
                "value": {
                    "enum": [
                        "Creating",
                        "Succeeded",
                        "Failed",
                        "Deleting"
                    ]
                }
            }
        },
        "provisioningError": {
            "parameters": {
                "provisioningErrorCode": {
                    "enum": [
                        "BadSourceType",
                        "BadPIRSource",
                        "BadISOSource",
                        "BadCustomizerType",
                        "NoCustomizerShellScript",
                        "ServerError",
                        "Other"
                    ]
                },
                "message": {
                    "type": "string"
                }
            }
        },
        "lastRunStatus": {
            "parameters": {
                "startTime": {},
                "endTime": {},
                "runState": {
                    "enum": [
                        "ready",
                        "running",
                        "succeeded",
                        "partiallySucceeded",
                        "failed"
                    ]
                },
                "runSubState": {
                    "enum": [
                        "queued",
                        "building",
                        "customizing",
                        "distributing"
                    ]
                },
                "message": {
                    "type": "string"
                }
            }
        }
    },
    "$resources": {
        "imageTemplates": {
            "source": {
                "$ref": "#/$keywords/source"
            },
            "customize": {
                "$ref": "#/$keywords/customize"
            },
            "distribute": {
                "$ref": "#/$keywords/distribute"
            },
            "provisioningState": {
                "$ref": "#/$keywords/provisioningState"
            },
            "provisioningError": {
                "$ref": "#/$keywords/provisioningError"
            },
            "lastRunStatus": {
                "$ref": "#/$keywords/lastRunStatus"
            }
        }
    }
}