{
  "$keywords": {
    "replicaCount": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 1
        }
      }
    },
    "partitionCount": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 1
        }
      }
    },
    "hostingMode": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "default",
            "highDensity"
          ]
        }
      }
    },
    "publicNetworkAccess": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "enabled",
            "disabled"
          ]
        }
      }
    },
    "networkRuleSet": {
      "body": {
        "ipRules": {
          "$ref": "#/$keywords/ipRules"
        }
      }
    },
    "ipRules": {
      "body": {
        "value": {
          "$ref": "#/$keywords/value"
        }
      },
      "array": true
    },
    "value": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "privateEndpoint": {
      "body": {
        "id": {
          "$ref": "#/$keywords/id"
        }
      }
    },
    "id": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "privateLinkServiceConnectionState": {
      "body": {
        "status": {
          "$ref": "#/$keywords/status"
        },
        "description": {
          "$ref": "#/$keywords/description"
        },
        "actionsRequired": {
          "$ref": "#/$keywords/actionsRequired"
        }
      }
    },
    "status": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Pending",
            "Approved",
            "Rejected",
            "Disconnected"
          ]
        }
      }
    },
    "description": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "actionsRequired": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "default": "None"
        }
      }
    }
  },
  "$resources": {
    "searchServices": {
      "replicaCount": {
        "$ref": "#/$keywords/replicaCount"
      },
      "partitionCount": {
        "$ref": "#/$keywords/partitionCount"
      },
      "hostingMode": {
        "$ref": "#/$keywords/hostingMode"
      },
      "publicNetworkAccess": {
        "$ref": "#/$keywords/publicNetworkAccess"
      },
      "networkRuleSet": {
        "$ref": "#/$keywords/networkRuleSet"
      }
    },
    "searchServices/privateEndpointConnections": {
      "privateEndpoint": {
        "$ref": "#/$keywords/privateEndpoint"
      },
      "privateLinkServiceConnectionState": {
        "$ref": "#/$keywords/privateLinkServiceConnectionState"
      }
    }
  }
}
