{
  "$keywords": {
    "isAutoInflateEnabled": {
      "propertyParameters": {
        "value": {
          "type": "bool",
          "default": false
        }
      }
    },
    "maximumThroughputUnits": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "kafkaEnabled": {
      "propertyParameters": {
        "value": {
          "type": "bool",
          "default": false
        }
      }
    },
    "rights": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Manage",
            "Send",
            "Listen"
          ]
        }
      },
      "array": true
    },
    "defaultAction": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Allow",
            "Deny"
          ]
        }
      }
    },
    "virtualNetworkRules": {
      "body": {
        "subnet": {
          "$ref": "#/$keywords/subnet"
