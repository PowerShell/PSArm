{
  "$keywords": {
    "alias": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "status": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Disabled",
            "Enabled"
          ]
        }
      }
    },
    "sourceRepository": {
      "body": {
        "sourceControlType": {
          "$ref": "#/$keywords/sourceControlType"
        },
        "repositoryUrl": {
          "$ref": "#/$keywords/repositoryUrl"
        },
        "isCommitTriggerEnabled": {
          "$ref": "#/$keywords/isCommitTriggerEnabled"
        },
        "sourceControlAuthProperties": {
          "$ref": "#/$keywords/sourceControlAuthProperties"
        }
      }
    },
    "sourceControlType": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Github",
            "VisualStudioTeamService"
          ]
        }
      }
    },
    "repositoryUrl": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "isCommitTriggerEnabled": {
      "propertyParameters": {
        "value": {
          "type": "bool",
          "default": false
        }
      }
    },
    "sourceControlAuthProperties": {
      "body": {
        "tokenType": {
          "$ref": "#/$keywords/tokenType"
        },
        "token": {
          "$ref": "#/$keywords/token"
        },
        "refreshToken": {
          "$ref": "#/$keywords/refreshToken"
        },
        "scope": {
          "$ref": "#/$keywords/scope"
        },
        "expiresIn": {
          "$ref": "#/$keywords/expiresIn"
        }
      }
    },
    "tokenType": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "PAT",
            "OAuth"
          ]
        }
      }
    },
    "token": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "refreshToken": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "scope": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "expiresIn": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "platform": {
      "body": {
        "osType": {
          "$ref": "#/$keywords/osType"
        },
        "cpu": {
          "$ref": "#/$keywords/cpu"
        }
      }
    },
    "osType": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Windows",
            "Linux"
          ]
        }
      }
    },
    "cpu": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "timeout": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 3600
        }
      }
    }
  },
  "$resources": {
    "registries/buildTasks": {
      "alias": {
        "$ref": "#/$keywords/alias"
      },
      "status": {
        "$ref": "#/$keywords/status"
      },
      "sourceRepository": {
        "$ref": "#/$keywords/sourceRepository"
      },
      "platform": {
        "$ref": "#/$keywords/platform"
      },
      "timeout": {
        "$ref": "#/$keywords/timeout"
      }
    },
    "registries/buildTasks/steps": {}
  }
}
