{
  "$keywords": {
    "storageModelType": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Invalid",
            "GeoRedundant",
            "LocallyRedundant"
          ]
        }
      }
    },
    "storageType": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Invalid",
            "GeoRedundant",
            "LocallyRedundant"
          ]
        }
      }
    },
    "storageTypeState": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Invalid",
            "Locked",
            "Unlocked"
          ]
        }
      }
    },
    "friendlyName": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "backupManagementType": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Invalid",
            "AzureIaasVM",
            "MAB",
            "DPM",
            "AzureSql",
            "AzureBackupServer",
            "AzureWorkload",
            "AzureStorage",
            "DefaultBackup"
          ]
        }
      }
    },
    "registrationStatus": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "healthStatus": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    }
  },
  "$resources": {
    "vaults/backupstorageconfig": {
      "storageModelType": {
        "$ref": "#/$keywords/storageModelType"
      },
      "storageType": {
        "$ref": "#/$keywords/storageType"
      },
      "storageTypeState": {
        "$ref": "#/$keywords/storageTypeState"
      }
    },
    "vaults/backupFabrics/protectionContainers": {
      "friendlyName": {
        "$ref": "#/$keywords/friendlyName"
      },
      "backupManagementType": {
        "$ref": "#/$keywords/backupManagementType"
      },
      "registrationStatus": {
        "$ref": "#/$keywords/registrationStatus"
      },
      "healthStatus": {
        "$ref": "#/$keywords/healthStatus"
      }
    }
  }
}
