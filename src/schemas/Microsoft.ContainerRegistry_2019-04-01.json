{
    "$keywords": {
        "provisioningState": {
            "propertyParameters": {
                "value": {
                    "enum": [
                        "Creating",
                        "Updating",
                        "Deleting",
                        "Succeeded",
                        "Failed",
                        "Canceled"
                    ]
                }
            }
        },
        "creationDate": {
            "propertyParameters": {
                "value": {}
            }
        },
        "status": {
            "propertyParameters": {
                "value": {
                    "enum": [
                        "Disabled",
                        "Enabled"
                    ]
                }
            }
        },
        "platform": {
            "parameters": {
                "os": {
                    "enum": [
                        "Windows",
                        "Linux"
                    ]
                },
                "architecture": {
                    "enum": [
                        "amd64",
                        "x86",
                        "arm"
                    ]
                },
                "variant": {
                    "enum": [
                        "v6",
                        "v7",
                        "v8"
                    ]
                }
            }
        },
        "agentConfiguration": {
            "parameters": {
                "cpu": {
                    "type": "int"
                }
            }
        },
        "timeout": {
            "propertyParameters": {
                "value": {
                    "type": "int"
                }
            }
        },
        "step": {
            "parameters": {
                "type": {
                    "enum": [
                        "Docker",
                        "FileTask",
                        "EncodedTask"
                    ]
                },
                "contextPath": {
                    "type": "string"
                },
                "contextAccessToken": {
                    "type": "string"
                }
            },
            "body": {
                "baseImageDependencies": {
                    "$ref": "#/$keywords/baseImageDependencies"
                }
            }
        },
        "baseImageDependencies": {
            "parameters": {
                "type": {
                    "enum": [
                        "BuildTime",
                        "RunTime"
                    ]
                },
                "registry": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                },
                "digest": {
                    "type": "string"
                }
            },
            "array": true
        },
        "trigger": {
            "body": {
                "timerTriggers": {
                    "$ref": "#/$keywords/timerTriggers"
                },
                "sourceTriggers": {
                    "$ref": "#/$keywords/sourceTriggers"
                },
                "baseImageTrigger": {
                    "$ref": "#/$keywords/baseImageTrigger"
                }
            }
        },
        "timerTriggers": {
            "parameters": {
                "schedule": {
                    "type": "string"
                },
                "status": {
                    "enum": [
                        "Disabled",
                        "Enabled"
                    ]
                },
                "name": {
                    "type": "string"
                }
            },
            "array": true
        },
        "sourceTriggers": {
            "parameters": {
                "status": {
                    "enum": [
                        "Disabled",
                        "Enabled"
                    ]
                },
                "name": {
                    "type": "string"
                }
            },
            "body": {
                "sourceRepository": {
                    "$ref": "#/$keywords/sourceRepository"
                },
                "sourceTriggerEvents": {
                    "$ref": "#/$keywords/sourceTriggerEvents"
                }
            },
            "array": true
        },
        "sourceRepository": {
            "parameters": {
                "sourceControlType": {
                    "enum": [
                        "Github",
                        "VisualStudioTeamService"
                    ]
                },
                "repositoryUrl": {
                    "type": "string"
                },
                "branch": {
                    "type": "string"
                }
            },
            "body": {
                "sourceControlAuthProperties": {
                    "$ref": "#/$keywords/sourceControlAuthProperties"
                }
            }
        },
        "sourceControlAuthProperties": {
            "parameters": {
                "tokenType": {
                    "enum": [
                        "PAT",
                        "OAuth"
                    ]
                },
                "token": {
                    "type": "string"
                },
                "refreshToken": {
                    "type": "string"
                },
                "scope": {
                    "type": "string"
                },
                "expiresIn": {
                    "type": "int"
                }
            }
        },
        "sourceTriggerEvents": {
            "propertyParameters": {
                "value": {
                    "enum": [
                        "commit",
                        "pullrequest"
                    ]
                }
            },
            "array": true
        },
        "baseImageTrigger": {
            "parameters": {
                "baseImageTriggerType": {
                    "enum": [
                        "All",
                        "Runtime"
                    ]
                },
                "status": {
                    "enum": [
                        "Disabled",
                        "Enabled"
                    ]
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "credentials": {
            "parameters": {
                "customRegistries": {}
            },
            "body": {
                "sourceRegistry": {
                    "$ref": "#/$keywords/sourceRegistry"
                }
            }
        },
        "sourceRegistry": {
            "parameters": {
                "loginMode": {
                    "enum": [
                        "None",
                        "Default"
                    ]
                }
            }
        }
    },
    "$resources": {
        "registries/tasks": {
            "provisioningState": {
                "$ref": "#/$keywords/provisioningState"
            },
            "creationDate": {
                "$ref": "#/$keywords/creationDate"
            },
            "status": {
                "$ref": "#/$keywords/status"
            },
            "platform": {
                "$ref": "#/$keywords/platform"
            },
            "agentConfiguration": {
                "$ref": "#/$keywords/agentConfiguration"
            },
            "timeout": {
                "$ref": "#/$keywords/timeout"
            },
            "step": {
                "$ref": "#/$keywords/step"
            },
            "trigger": {
                "$ref": "#/$keywords/trigger"
            },
            "credentials": {
                "$ref": "#/$keywords/credentials"
            }
        }
    }
}