          "$ref": "#/$keywords/sourceDatabaseName"
        }
      },
      "array": true
    },
    "sourceDatabaseName": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "extension": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "filePath": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "mediaType": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    }
