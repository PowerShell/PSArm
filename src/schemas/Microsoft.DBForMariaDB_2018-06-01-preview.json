{
    "$keywords": {
        "version": {
            "propertyParameters": {
                "value": {
                    "enum": [
                        "5.6",
                        "5.7"
                    ]
                }
            }
        },
        "sslEnforcement": {
            "propertyParameters": {
                "value": {}
            }
        },
        "storageProfile": {
            "parameters": {
                "backupRetentionDays": {
                    "type": "int"
                },
                "geoRedundantBackup": {
                    "enum": [
                        "Enabled",
                        "Disabled"
                    ]
                },
                "storageMB": {
                    "type": "int"
                },
                "storageAutogrow": {
                    "enum": [
                        "Enabled",
                        "Disabled"
                    ]
                }
            }
        },
        "createMode": {
            "propertyParameters": {
                "value": {
                    "enum": [
