{
    "$keywords": {
        "provisioningState": {
            "propertyParameters": {
                "value": {
                    "enum": [
                        "AdminUpdating",
                        "Creating",
                        "Deleting",
                        "Failed",
                        "Succeeded",
                        "Updating"
                    ]
                }
            }
        },
        "clusterProfile": {
            "parameters": {
                "pullSecret": {
                    "type": "string"
                },
                "domain": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                },
                "resourceGroupId": {
                    "type": "string"
                }
            }
        },
        "consoleProfile": {
            "parameters": {
                "url": {
                    "type": "string"
                }
            }
        },
        "servicePrincipalProfile": {
            "parameters": {
                "clientId": {
                    "type": "string"
                },
                "clientSecret": {
                    "type": "string"
                }
            }
        },
        "networkProfile": {
            "parameters": {
                "podCidr": {
                    "type": "string"
                },
                "serviceCidr": {
                    "type": "string"
                }
            }
        },
        "masterProfile": {
            "parameters": {
                "vmSize": {
                    "enum": [
                        "Standard_D2s_v3",
                        "Standard_D4s_v3",
                        "Standard_D8s_v3"
                    ]
                },
                "subnetId": {
                    "type": "string"
                }
            }
        },
        "workerProfiles": {
            "parameters": {
                "name": {
                    "type": "string"
                },
                "vmSize": {
                    "enum": [
                        "Standard_D2s_v3",
                        "Standard_D4s_v3",
                        "Standard_D8s_v3"
                    ]
                },
                "diskSizeGB": {
                    "type": "int"
                },
                "subnetId": {
                    "type": "string"
                },
                "count": {
                    "type": "int"
                }
            },
            "array": true
        },
        "apiserverProfile": {
            "parameters": {
                "visibility": {
                    "enum": [
                        "Private",
                        "Public"
                    ]
                },
                "url": {
                    "type": "string"
                },
                "ip": {
                    "type": "string"
                }
            }
        },
        "ingressProfiles": {
            "parameters": {
                "name": {
                    "type": "string"
                },
                "visibility": {
                    "enum": [
                        "Private",
                        "Public"
                    ]
                },
                "ip": {
                    "type": "string"
                }
            },
            "array": true
        }
    },
    "$resources": {
        "openShiftClusters": {
            "provisioningState": {
                "$ref": "#/$keywords/provisioningState"
            },
            "clusterProfile": {
                "$ref": "#/$keywords/clusterProfile"
            },
            "consoleProfile": {
                "$ref": "#/$keywords/consoleProfile"
            },
            "servicePrincipalProfile": {
                "$ref": "#/$keywords/servicePrincipalProfile"
            },
            "networkProfile": {
                "$ref": "#/$keywords/networkProfile"
            },
            "masterProfile": {
                "$ref": "#/$keywords/masterProfile"
            },
            "workerProfiles": {
                "$ref": "#/$keywords/workerProfiles"
            },
            "apiserverProfile": {
                "$ref": "#/$keywords/apiserverProfile"
            },
            "ingressProfiles": {
                "$ref": "#/$keywords/ingressProfiles"
            }
        }
    }
}