{
  "$keywords": {
    "profileStatus": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Enabled",
            "Disabled"
          ]
        }
      }
    },
    "trafficRoutingMethod": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Performance",
            "Priority",
            "Weighted",
            "Geographic"
          ]
        }
      }
    },
    "dnsConfig": {
      "body": {
        "relativeName": {
          "$ref": "#/$keywords/relativeName"
        },
        "ttl": {
          "$ref": "#/$keywords/ttl"
        }
      }
    },
    "relativeName": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "ttl": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "monitorConfig": {
      "body": {
        "profileMonitorStatus": {
          "$ref": "#/$keywords/profileMonitorStatus"
        },
        "protocol": {
          "$ref": "#/$keywords/protocol"
        },
        "port": {
          "$ref": "#/$keywords/port"
        },
        "path": {
          "$ref": "#/$keywords/path"
        },
        "intervalInSeconds": {
          "$ref": "#/$keywords/intervalInSeconds"
        },
        "timeoutInSeconds": {
          "$ref": "#/$keywords/timeoutInSeconds"
        },
        "toleratedNumberOfFailures": {
          "$ref": "#/$keywords/toleratedNumberOfFailures"
        }
      }
    },
    "profileMonitorStatus": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "CheckingEndpoints",
            "Online",
            "Degraded",
            "Disabled",
            "Inactive"
          ]
        }
      }
    },
    "protocol": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "HTTP",
            "HTTPS",
            "TCP"
          ]
        }
      }
    },
    "port": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "path": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "intervalInSeconds": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "timeoutInSeconds": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "toleratedNumberOfFailures": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "endpoints": {
      "body": {
        "properties": {
          "$ref": "#/$keywords/properties"
        }
      },
      "array": true
    },
    "properties": {
      "body": {
        "targetResourceId": {
          "$ref": "#/$keywords/targetResourceId"
        },
        "target": {
          "$ref": "#/$keywords/target"
        },
        "endpointStatus": {
          "$ref": "#/$keywords/endpointStatus"
        },
        "weight": {
          "$ref": "#/$keywords/weight"
        },
        "priority": {
          "$ref": "#/$keywords/priority"
        },
        "endpointLocation": {
          "$ref": "#/$keywords/endpointLocation"
        },
        "endpointMonitorStatus": {
          "$ref": "#/$keywords/endpointMonitorStatus"
        },
        "minChildEndpoints": {
          "$ref": "#/$keywords/minChildEndpoints"
        },
        "geoMapping": {
          "$ref": "#/$keywords/geoMapping"
        }
      }
    },
    "targetResourceId": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "target": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "endpointStatus": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Enabled",
            "Disabled"
          ]
        }
      }
    },
    "weight": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "priority": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "endpointLocation": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "endpointMonitorStatus": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "CheckingEndpoint",
            "Online",
            "Degraded",
            "Disabled",
            "Inactive",
            "Stopped"
          ]
        }
      }
    },
    "minChildEndpoints": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "geoMapping": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      },
      "array": true
    }
  },
  "$resources": {
    "trafficmanagerprofiles": {
      "profileStatus": {
        "$ref": "#/$keywords/profileStatus"
      },
      "trafficRoutingMethod": {
        "$ref": "#/$keywords/trafficRoutingMethod"
      },
      "dnsConfig": {
        "$ref": "#/$keywords/dnsConfig"
      },
      "monitorConfig": {
        "$ref": "#/$keywords/monitorConfig"
      },
      "endpoints": {
        "$ref": "#/$keywords/endpoints"
      }
    }
  }
}
