            },
            "serviceLevel": {
                "$ref": "#/$keywords/serviceLevel"
            },
            "usageThreshold": {
                "$ref": "#/$keywords/usageThreshold"
            },
            "exportPolicy": {
                "$ref": "#/$keywords/exportPolicy"
            },
            "protocolTypes": {
                "$ref": "#/$keywords/protocolTypes"
            },
            "provisioningState": {
                "$ref": "#/$keywords/provisioningState_3"
            },
            "snapshotId": {
                "$ref": "#/$keywords/snapshotId"
            },
            "baremetalTenantId": {
                "$ref": "#/$keywords/baremetalTenantId"
            },
            "subnetId": {
                "$ref": "#/$keywords/subnetId"
            },
            "mountTargets": {
                "$ref": "#/$keywords/mountTargets"
            },
            "volumeType": {
