{
  "$keywords": {
    "storageModelType": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Invalid",
            "GeoRedundant",
            "LocallyRedundant"
          ]
        }
      }
    },
    "storageType": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Invalid",
            "GeoRedundant",
            "LocallyRedundant"
          ]
        }
      }
    },
    "storageTypeState": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Invalid",
            "Locked",
            "Unlocked"
          ]
        }
      }
    },
    "enhancedSecurityState": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Invalid",
            "Enabled",
            "Disabled"
          ]
        }
      }
    },
    "softDeleteFeatureState": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Invalid",
            "Enabled",
            "Disabled"
          ]
        }
      }
    },
    "protectedItemsCount": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "backupManagementType": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Invalid",
            "AzureIaasVM",
            "MAB",
            "DPM",
            "AzureBackupServer",
            "AzureSql",
            "AzureStorage",
            "AzureWorkload",
            "DefaultBackup"
          ]
        }
      }
    },
    "workloadType": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Invalid",
            "VM",
            "FileFolder",
            "AzureSqlDb",
            "SQLDB",
            "Exchange",
            "Sharepoint",
            "VMwareVM",
            "SystemState",
            "Client",
            "GenericDataSource",
            "SQLDataBase",
            "AzureFileShare",
            "SAPHanaDatabase",
            "SAPAseDatabase"
          ]
        }
      }
    },
    "containerName": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "sourceResourceId": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "policyId": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "lastRecoveryPoint": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "backupSetName": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "createMode": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Invalid",
            "Default",
            "Recover"
          ]
        }
      }
    },
    "deferredDeleteTimeInUTC": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "isScheduledForDeferredDelete": {
      "propertyParameters": {
        "value": {
          "type": "bool",
          "default": false
        }
      }
    },
    "deferredDeleteTimeRemaining": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "isDeferredDeleteScheduleUpcoming": {
      "propertyParameters": {
        "value": {
          "type": "bool",
          "default": false
        }
      }
    },
    "isRehydrate": {
      "propertyParameters": {
        "value": {
          "type": "bool",
          "default": false
        }
      }
    }
  },
  "$resources": {
    "vaults/backupconfig": {
      "storageModelType": {
        "$ref": "#/$keywords/storageModelType"
      },
      "storageType": {
        "$ref": "#/$keywords/storageType"
      },
      "storageTypeState": {
        "$ref": "#/$keywords/storageTypeState"
      },
      "enhancedSecurityState": {
        "$ref": "#/$keywords/enhancedSecurityState"
      },
      "softDeleteFeatureState": {
        "$ref": "#/$keywords/softDeleteFeatureState"
      }
    },
    "vaults/backupPolicies": {
      "protectedItemsCount": {
        "$ref": "#/$keywords/protectedItemsCount"
      }
    },
    "vaults/backupFabrics/protectionContainers/protectedItems": {
      "backupManagementType": {
        "$ref": "#/$keywords/backupManagementType"
      },
      "workloadType": {
        "$ref": "#/$keywords/workloadType"
      },
      "containerName": {
        "$ref": "#/$keywords/containerName"
      },
      "sourceResourceId": {
        "$ref": "#/$keywords/sourceResourceId"
      },
      "policyId": {
        "$ref": "#/$keywords/policyId"
      },
      "lastRecoveryPoint": {
        "$ref": "#/$keywords/lastRecoveryPoint"
      },
      "backupSetName": {
        "$ref": "#/$keywords/backupSetName"
      },
      "createMode": {
        "$ref": "#/$keywords/createMode"
      },
      "deferredDeleteTimeInUTC": {
        "$ref": "#/$keywords/deferredDeleteTimeInUTC"
      },
      "isScheduledForDeferredDelete": {
        "$ref": "#/$keywords/isScheduledForDeferredDelete"
      },
      "deferredDeleteTimeRemaining": {
        "$ref": "#/$keywords/deferredDeleteTimeRemaining"
      },
      "isDeferredDeleteScheduleUpcoming": {
        "$ref": "#/$keywords/isDeferredDeleteScheduleUpcoming"
      },
      "isRehydrate": {
        "$ref": "#/$keywords/isRehydrate"
      }
    }
  }
}
