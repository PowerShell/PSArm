        "sizeLimitInBytes": {
          "$ref": "#/$keywords/sizeLimitInBytes"
        },
        "destination": {
          "$ref": "#/$keywords/destination"
        },
        "skipEmptyArchives": {
          "$ref": "#/$keywords/skipEmptyArchives"
        }
      }
    },
    "enabled": {
      "propertyParameters": {
        "value": {
          "type": "bool",
          "default": false
