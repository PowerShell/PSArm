                }
            }
        },
        "enableCustomerAnalytics": {
            "propertyParameters": {
                "value": {
                    "type": "bool"
                }
            }
        },
        "logAnalyticsWorkspaceId": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "logAnalyticsWorkspaceSharedKey": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        }
    },
    "$resources": {
