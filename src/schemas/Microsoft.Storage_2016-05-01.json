{
  "$keywords": {
    "customDomain": {
      "body": {
        "name": {
          "$ref": "#/$keywords/name"
        },
        "useSubDomainName": {
          "$ref": "#/$keywords/useSubDomainName"
        }
      }
    },
    "name": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "useSubDomainName": {
      "propertyParameters": {
        "value": {
          "type": "bool",
          "default": false
        }
      }
    },
    "encryption": {
      "body": {
        "services": {
          "$ref": "#/$keywords/services"
        },
        "keySource": {
          "$ref": "#/$keywords/keySource"
        }
      }
    },
    "services": {
      "body": {
        "blob": {
          "$ref": "#/$keywords/blob"
        }
      }
    },
    "blob": {
      "body": {
        "enabled": {
          "$ref": "#/$keywords/enabled"
        }
      }
    },
    "enabled": {
      "propertyParameters": {
        "value": {
          "type": "bool",
          "default": false
        }
      }
    },
    "keySource": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Microsoft.Storage"
          ]
        }
      }
    },
    "accessTier": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Hot",
            "Cool"
          ]
        }
      }
    }
  },
  "$resources": {
    "storageAccounts": {
      "customDomain": {
        "$ref": "#/$keywords/customDomain"
      },
      "encryption": {
        "$ref": "#/$keywords/encryption"
      },
      "accessTier": {
        "$ref": "#/$keywords/accessTier"
      }
    }
  }
}
