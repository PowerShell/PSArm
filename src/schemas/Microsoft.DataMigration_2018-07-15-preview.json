            "SQLMI",
            "AzureDbForMySql",
            "AzureDbForPostgreSql",
            "MongoDb",
            "Unknown"
          ]
        }
      }
    },
    "sourceConnectionInfo": {
      "body": {
        "userName": {
          "$ref": "#/$keywords/userName"
        },
        "password": {
          "$ref": "#/$keywords/password"
        }
      }
    },
    "userName": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "password": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "targetConnectionInfo": {
      "body": {
        "userName": {
          "$ref": "#/$keywords/userName"
        },
        "password": {
          "$ref": "#/$keywords/password"
        }
      }
    },
    "databasesInfo": {
      "body": {
