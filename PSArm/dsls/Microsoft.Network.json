{
    "networkInterfaces": {
        "IpConfiguration": {
            "kind": "array",
            "items": {
                "kind": "block",
                "parameters": [
                    {
                        "name": "Name",
                        "type": "string"
                    }
                ],
                "body": {
                    "Subnet": {
                        "kind": "bodycommand",
                        "parameters": [
                            {
                                "name": "Id",
                                "type": "string"
                            }
                        ]
                    },
                    "PrivateIPAllocationMethod": {
                        "kind": "command",
                        "parameters": [
                            {
                                "name": "Method",
                                "type": "string",
                                "enum": [ "Static", "Dynamic" ]
                            }
                        ]
                    }
                }
            }
        }
    }
}
