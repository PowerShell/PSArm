{
    "$keywords": {
        "hardwareProfile": {
            "parameters": {
                "hardwareType": {
                    "enum": [
                        "Cisco_UCS",
                        "HPE"
                    ]
                },
                "hanaInstanceSize": {
                    "enum": [
                        "S72m",
                        "S144m",
                        "S72",
                        "S144",
                        "S192",
                        "S192m",
                        "S192xm",
                        "S96",
                        "S112",
                        "S224",
                        "S224m",
                        "S224om",
                        "S224oo",
                        "S224oom",
                        "S224ooo",
                        "S384",
                        "S384m",
                        "S384xm",
                        "S384xxm",
                        "S448",
                        "S448m",
                        "S448om",
                        "S448oo",
                        "S448oom",
                        "S448ooo",
                        "S576m",
                        "S576xm",
                        "S672",
                        "S672m",
                        "S672om",
                        "S672oo",
                        "S672oom",
                        "S672ooo",
                        "S768",
                        "S768m",
                        "S768xm",
                        "S896",
                        "S896m",
                        "S896om",
                        "S896oo",
                        "S896oom",
                        "S896ooo",
                        "S960m"
                    ]
                }
            }
        },
        "storageProfile": {
            "parameters": {
                "nfsIpAddress": {
                    "type": "string"
                }
            },
            "body": {
                "osDisks": {
                    "$ref": "#/$keywords/osDisks"
                }
            }
        },
        "osDisks": {
            "parameters": {
                "name": {
                    "type": "string"
                },
                "diskSizeGB": {
                    "type": "int"
                },
                "lun": {
                    "type": "int"
                }
            },
            "array": true
        },
        "osProfile": {
            "parameters": {
                "computerName": {
                    "type": "string"
                },
                "osType": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                },
                "sshPublicKey": {
                    "type": "string"
                }
            }
        },
        "networkProfile": {
            "parameters": {
                "circuitId": {
                    "type": "string"
                }
            },
            "body": {
                "networkInterfaces": {
                    "$ref": "#/$keywords/networkInterfaces"
                }
            }
        },
        "networkInterfaces": {
            "parameters": {
                "ipAddress": {
                    "type": "string"
                }
            },
            "array": true
        },
        "hanaInstanceId": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "powerState": {
            "propertyParameters": {
                "value": {
                    "enum": [
                        "starting",
                        "started",
                        "stopping",
                        "stopped",
                        "restarting",
                        "unknown"
                    ]
                }
            }
        },
        "proximityPlacementGroup": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "hwRevision": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "partnerNodeId": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "provisioningState": {
            "propertyParameters": {
                "value": {
                    "enum": [
                        "Accepted",
                        "Creating",
                        "Updating",
                        "Failed",
                        "Succeeded",
                        "Deleting",
                        "Migrating"
                    ]
                }
            }
        },
        "hanaSubnet": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "hanaHostname": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "hanaDbName": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "hanaDbSqlPort": {
            "propertyParameters": {
                "value": {
                    "type": "int"
                }
            }
        },
        "hanaDbUsername": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "hanaDbPassword": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "hanaDbPasswordKeyVaultUrl": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "hanaDbCredentialsMsiId": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "keyVaultId": {
