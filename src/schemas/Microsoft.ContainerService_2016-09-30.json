{
    "$keywords": {
        "provisioningState": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "orchestratorProfile": {
            "parameters": {
                "orchestratorType": {}
            }
        },
        "customProfile": {
            "parameters": {
                "orchestrator": {
                    "type": "string"
                }
            }
        },
        "servicePrincipalProfile": {
            "parameters": {
                "clientId": {
                    "type": "string"
                },
                "secret": {
                    "type": "string"
                }
            }
        },
        "masterProfile": {
            "parameters": {
                "count": {
                    "enum": [
                        1,
                        3,
                        5
                    ]
                },
                "dnsPrefix": {
                    "type": "string"
                },
                "fqdn": {
                    "type": "string"
                }
            }
        },
        "agentPoolProfiles": {
            "parameters": {
                "name": {
                    "type": "string"
                },
                "count": {
                    "type": "int"
                },
                "vmSize": {
                    "enum": [
                        "Standard_A0",
                        "Standard_A1",
                        "Standard_A2",
                        "Standard_A3",
                        "Standard_A4",
                        "Standard_A5",
                        "Standard_A6",
                        "Standard_A7",
                        "Standard_A8",
                        "Standard_A9",
                        "Standard_A10",
                        "Standard_A11",
                        "Standard_D1",
                        "Standard_D2",
                        "Standard_D3",
                        "Standard_D4",
                        "Standard_D11",
                        "Standard_D12",
                        "Standard_D13",
                        "Standard_D14",
                        "Standard_D1_v2",
                        "Standard_D2_v2",
                        "Standard_D3_v2",
                        "Standard_D4_v2",
                        "Standard_D5_v2",
                        "Standard_D11_v2",
                        "Standard_D12_v2",
                        "Standard_D13_v2",
                        "Standard_D14_v2",
                        "Standard_G1",
                        "Standard_G2",
                        "Standard_G3",
                        "Standard_G4",
                        "Standard_G5",
                        "Standard_DS1",
                        "Standard_DS2",
                        "Standard_DS3",
                        "Standard_DS4",
                        "Standard_DS11",
                        "Standard_DS12",
                        "Standard_DS13",
                        "Standard_DS14",
                        "Standard_GS1",
                        "Standard_GS2",
                        "Standard_GS3",
                        "Standard_GS4",
                        "Standard_GS5"
                    ]
                },
                "dnsPrefix": {
                    "type": "string"
                },
                "fqdn": {
                    "type": "string"
                }
            },
            "array": true
        },
        "windowsProfile": {
            "parameters": {
                "adminUsername": {
                    "type": "string",
                    "pattern": "^[a-zA-Z0-9]+([._]?[a-zA-Z0-9]+)*$"
                },
                "adminPassword": {
                    "type": "string",
                    "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*[!@#$%\\^&\\*\\(\\)])[a-zA-Z\\d!@#$%\\^&\\*\\(\\)]{12,123}$"
                }
            }
        },
        "linuxProfile": {
            "parameters": {
                "adminUsername": {
                    "type": "string",
                    "pattern": "^[a-z][a-z0-9_-]*$"
                }
            },
            "body": {
                "ssh": {
                    "$ref": "#/$keywords/ssh"
                }
            }
        },
        "ssh": {
            "body": {
                "publicKeys": {
                    "$ref": "#/$keywords/publicKeys"
                }
            }
        },
        "publicKeys": {
            "parameters": {
                "keyData": {
                    "type": "string"
                }
            },
            "array": true
        },
        "diagnosticsProfile": {
            "body": {
                "vmDiagnostics": {
                    "$ref": "#/$keywords/vmDiagnostics"
                }
            }
        },
        "vmDiagnostics": {
            "parameters": {
                "enabled": {
                    "type": "bool"
                },
                "storageUri": {
                    "type": "string"
                }
            }
        }
    },
    "$resources": {
        "containerServices": {
            "provisioningState": {
                "$ref": "#/$keywords/provisioningState"
            },
            "orchestratorProfile": {
                "$ref": "#/$keywords/orchestratorProfile"
            },
            "customProfile": {
                "$ref": "#/$keywords/customProfile"
            },
            "servicePrincipalProfile": {
                "$ref": "#/$keywords/servicePrincipalProfile"
            },
            "masterProfile": {
                "$ref": "#/$keywords/masterProfile"
            },
            "agentPoolProfiles": {
                "$ref": "#/$keywords/agentPoolProfiles"
            },
            "windowsProfile": {
                "$ref": "#/$keywords/windowsProfile"
            },
            "linuxProfile": {
                "$ref": "#/$keywords/linuxProfile"
            },
            "diagnosticsProfile": {
                "$ref": "#/$keywords/diagnosticsProfile"
            }
        }
    }
}