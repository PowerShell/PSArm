{
    "$keywords": {
        "provisioningState": {
            "propertyParameters": {
                "value": {
                    "enum": [
                        "Deleting",
                        "Succeeded",
                        "Creating",
                        "Accepted",
                        "Verifying",
                        "Updating",
                        "Failed",
                        "Canceled",
                        "Deprovisioned"
                    ]
                }
            }
        },
        "accessPolicies": {
            "parameters": {
                "objectId": {
                    "type": "string",
                    "pattern": "^(([0-9A-Fa-f]{8}[-]?(?:[0-9A-Fa-f]{4}[-]?){3}[0-9A-Fa-f]{12}){1})+$"
                }
            },
            "array": true
        },
        "cosmosDbConfiguration": {
            "parameters": {
                "offerThroughput": {
                    "type": "int"
                }
            }
        },
        "authenticationConfiguration": {
            "parameters": {
                "authority": {
                    "type": "string"
                },
                "audience": {
                    "type": "string"
                },
                "smartProxyEnabled": {
                    "type": "bool"
                }
            }
        },
        "corsConfiguration": {
            "parameters": {
                "maxAge": {
                    "type": "int"
                },
                "allowCredentials": {
                    "type": "bool"
                }
            },
            "body": {
                "origins": {
                    "$ref": "#/$keywords/origins"
                },
                "headers": {
                    "$ref": "#/$keywords/headers"
                },
                "methods": {
                    "$ref": "#/$keywords/methods"
                }
            }
        },
        "origins": {
            "propertyParameters": {
                "value": {
                    "type": "string",
                    "pattern": "^(?:(?:(?:[hH][tT][tT][pP](?:[sS]|))\\:\\/\\/(?:[a-zA-Z0-9-]+[.]?)+(?:\\:[0-9]{1,5})?|[*]))$"
                }
            },
            "array": true
        },
        "headers": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            },
            "array": true
        },
        "methods": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            },
            "array": true
        },
        "exportConfiguration": {
            "parameters": {
                "storageAccountName": {
                    "type": "string"
                }
            }
        }
    },
    "$resources": {
        "services": {
            "provisioningState": {
                "$ref": "#/$keywords/provisioningState"
            },
            "accessPolicies": {
                "$ref": "#/$keywords/accessPolicies"
            },
            "cosmosDbConfiguration": {
                "$ref": "#/$keywords/cosmosDbConfiguration"
            },
            "authenticationConfiguration": {
                "$ref": "#/$keywords/authenticationConfiguration"
            },
            "corsConfiguration": {
                "$ref": "#/$keywords/corsConfiguration"
            },
            "exportConfiguration": {
                "$ref": "#/$keywords/exportConfiguration"
            }
        }
    }
}