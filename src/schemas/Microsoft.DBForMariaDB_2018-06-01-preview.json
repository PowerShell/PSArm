{
  "$keywords": {
    "version": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "5.6",
            "5.7"
          ]
        }
      }
    },
    "sslEnforcement": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Enabled",
            "Disabled"
          ]
        }
      }
    },
    "storageProfile": {
      "body": {
        "backupRetentionDays": {
          "$ref": "#/$keywords/backupRetentionDays"
        },
        "geoRedundantBackup": {
          "$ref": "#/$keywords/geoRedundantBackup"
        },
        "storageMB": {
          "$ref": "#/$keywords/storageMB"
        },
        "storageAutogrow": {
          "$ref": "#/$keywords/storageAutogrow"
        }
      }
    },
    "backupRetentionDays": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "geoRedundantBackup": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Enabled",
            "Disabled"
          ]
        }
      }
    },
    "storageMB": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    },
    "storageAutogrow": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Enabled",
            "Disabled"
          ]
        }
      }
    },
    "startIpAddress": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "endIpAddress": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "virtualNetworkSubnetId": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "ignoreMissingVnetServiceEndpoint": {
      "propertyParameters": {
        "value": {
          "type": "bool",
          "default": false
        }
      }
    },
    "charset": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "collation": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "value": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "source": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "state": {
      "propertyParameters": {
        "value": {
          "type": "string",
          "enum": [
            "Enabled",
            "Disabled"
          ]
        }
      }
    },
    "disabledAlerts": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      },
      "array": true
    },
    "emailAddresses": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      },
      "array": true
    },
    "emailAccountAdmins": {
      "propertyParameters": {
        "value": {
          "type": "bool",
          "default": false
        }
      }
    },
    "storageEndpoint": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "storageAccountAccessKey": {
      "propertyParameters": {
        "value": {
          "type": "string"
        }
      }
    },
    "retentionDays": {
      "propertyParameters": {
        "value": {
          "type": "int",
          "default": 0
        }
      }
    }
  },
  "$resources": {
    "servers": {
      "version": {
        "$ref": "#/$keywords/version"
      },
      "sslEnforcement": {
        "$ref": "#/$keywords/sslEnforcement"
      },
      "storageProfile": {
        "$ref": "#/$keywords/storageProfile"
      }
    },
    "servers/firewallRules": {
      "startIpAddress": {
        "$ref": "#/$keywords/startIpAddress"
      },
      "endIpAddress": {
        "$ref": "#/$keywords/endIpAddress"
      }
    },
    "servers/virtualNetworkRules": {
      "virtualNetworkSubnetId": {
        "$ref": "#/$keywords/virtualNetworkSubnetId"
      },
      "ignoreMissingVnetServiceEndpoint": {
        "$ref": "#/$keywords/ignoreMissingVnetServiceEndpoint"
      }
    },
    "servers/databases": {
      "charset": {
        "$ref": "#/$keywords/charset"
      },
      "collation": {
        "$ref": "#/$keywords/collation"
      }
    },
    "servers/configurations": {
      "value": {
        "$ref": "#/$keywords/value"
      },
      "source": {
        "$ref": "#/$keywords/source"
      }
    },
    "servers/securityAlertPolicies": {
      "state": {
        "$ref": "#/$keywords/state"
      },
      "disabledAlerts": {
        "$ref": "#/$keywords/disabledAlerts"
      },
      "emailAddresses": {
        "$ref": "#/$keywords/emailAddresses"
      },
      "emailAccountAdmins": {
        "$ref": "#/$keywords/emailAccountAdmins"
      },
      "storageEndpoint": {
        "$ref": "#/$keywords/storageEndpoint"
      },
      "storageAccountAccessKey": {
        "$ref": "#/$keywords/storageAccountAccessKey"
      },
      "retentionDays": {
        "$ref": "#/$keywords/retentionDays"
      }
    }
  }
}
