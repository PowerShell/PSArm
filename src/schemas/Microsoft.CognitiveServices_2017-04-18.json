{
    "$keywords": {
        "provisioningState": {
            "propertyParameters": {
                "value": {
                    "enum": [
                        "Creating",
                        "ResolvingDNS",
                        "Moving",
                        "Deleting",
                        "Succeeded",
                        "Failed"
                    ]
                }
            }
        },
        "endpoint": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "internalId": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "capabilities": {
            "parameters": {
                "name": {
                    "type": "string"
                },
                "value": {
                    "type": "string"
                }
            },
            "array": true
        },
        "customSubDomainName": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "networkAcls": {
            "parameters": {
                "defaultAction": {
                    "enum": [
                        "Allow",
                        "Deny"
                    ]
                }
            },
            "body": {
                "ipRules": {
                    "$ref": "#/$keywords/ipRules"
                },
                "virtualNetworkRules": {
                    "$ref": "#/$keywords/virtualNetworkRules"
                }
            }
        },
        "ipRules": {
            "parameters": {
                "value": {
                    "type": "string"
                }
            },
            "array": true
        },
        "virtualNetworkRules": {
            "parameters": {
                "id": {
                    "type": "string"
                },
                "state": {
                    "type": "string"
                },
                "ignoreMissingVnetServiceEndpoint": {
                    "type": "bool"
                }
            },
            "array": true
        },
        "encryption": {
            "parameters": {
                "keySource": {
                    "enum": [
                        "Microsoft.CognitiveServices",
                        "Microsoft.KeyVault"
                    ]
                }
            },
            "body": {
                "keyVaultProperties": {
                    "$ref": "#/$keywords/keyVaultProperties"
                }
            }
        },
        "keyVaultProperties": {
            "parameters": {
                "keyName": {
                    "type": "string"
                },
                "keyVersion": {
                    "type": "string"
                },
                "keyVaultUri": {
                    "type": "string"
                }
            }
        },
        "userOwnedStorage": {
            "parameters": {
                "resourceId": {
                    "type": "string"
                }
            },
            "array": true
        },
        "privateEndpointConnections": {
            "parameters": {
                "id": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                }
            },
            "body": {
                "privateEndpoint": {
                    "$ref": "#/$keywords/privateEndpoint"
                },
                "privateLinkServiceConnectionState": {
                    "$ref": "#/$keywords/privateLinkServiceConnectionState"
                },
                "groupIds": {
                    "$ref": "#/$keywords/groupIds"
                }
            },
            "array": true
        },
        "privateEndpoint": {
            "parameters": {
                "id": {
                    "type": "string"
                }
            }
        },
        "privateLinkServiceConnectionState": {
            "parameters": {
                "status": {
                    "enum": [
                        "Pending",
                        "Approved",
                        "Rejected",
                        "Disconnected"
                    ]
                },
                "description": {
                    "type": "string"
                },
                "actionRequired": {
                    "type": "string"
                }
            }
        },
        "groupIds": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            },
            "array": true
        },
        "publicNetworkAccess": {
            "propertyParameters": {
                "value": {
                    "enum": [
                        "Enabled",
                        "Disabled"
                    ]
                }
            }
        },
        "apiProperties": {
            "parameters": {
                "qnaRuntimeEndpoint": {
                    "type": "string"
                },
                "statisticsEnabled": {
                    "type": "bool"
                },
                "eventHubConnectionString": {
                    "type": "string",
                    "pattern": "^( *)Endpoint=sb://(.*);( *)SharedAccessKeyName=(.*);( *)SharedAccessKey=(.*)$"
                },
                "storageAccountConnectionString": {
                    "type": "string",
                    "pattern": "^(( *)DefaultEndpointsProtocol=(http|https)( *);( *))?AccountName=(.*)AccountKey=(.*)EndpointSuffix=(.*)$"
                },
                "aadClientId": {
                    "type": "string"
                },
                "aadTenantId": {
                    "type": "string"
                },
                "superUser": {
                    "type": "string"
                },
                "websiteName": {
                    "type": "string"
                }
            }
        }
    },
    "$resources": {
        "accounts": {
            "provisioningState": {
                "$ref": "#/$keywords/provisioningState"
            },
            "endpoint": {
                "$ref": "#/$keywords/endpoint"
            },
            "internalId": {
                "$ref": "#/$keywords/internalId"
            },
            "capabilities": {
                "$ref": "#/$keywords/capabilities"
            },
            "customSubDomainName": {
                "$ref": "#/$keywords/customSubDomainName"
            },
            "networkAcls": {
                "$ref": "#/$keywords/networkAcls"
            },
            "encryption": {
                "$ref": "#/$keywords/encryption"
            },
            "userOwnedStorage": {
                "$ref": "#/$keywords/userOwnedStorage"
            },
            "privateEndpointConnections": {
                "$ref": "#/$keywords/privateEndpointConnections"
            },
            "publicNetworkAccess": {
                "$ref": "#/$keywords/publicNetworkAccess"
            },
            "apiProperties": {
                "$ref": "#/$keywords/apiProperties"
            }
        },
        "accounts/privateEndpointConnections": {
            "privateEndpoint": {
                "$ref": "#/$keywords/privateEndpoint"
            },
            "privateLinkServiceConnectionState": {
                "$ref": "#/$keywords/privateLinkServiceConnectionState"
            },
            "groupIds": {
                "$ref": "#/$keywords/groupIds"
            }
        }
    }
}