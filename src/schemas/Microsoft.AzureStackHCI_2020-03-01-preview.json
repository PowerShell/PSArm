            },
            "trialDaysRemaining": {
                "$ref": "#/$keywords/trialDaysRemaining"
            },
            "billingModel": {
                "$ref": "#/$keywords/billingModel"
            },
            "registrationTimestamp": {
                "$ref": "#/$keywords/registrationTimestamp"
            },
            "lastSyncTimestamp": {
                "$ref": "#/$keywords/lastSyncTimestamp"
            },
            "lastBillingTimestamp": {
                "$ref": "#/$keywords/lastBillingTimestamp"
            }
        }
