        "provisioningState": {
            "propertyParameters": {
                "value": {
                    "enum": [
                        "Succeeded",
                        "Failed",
                        "Canceled",
                        "Accepted",
                        "Provisioning"
                    ]
                }
            }
        },
        "status": {
            "propertyParameters": {
                "value": {
                    "enum": [
                        "NotYetRegistered",
                        "ConnectedRecently",
                        "NotConnectedRecently",
                        "Disconnected",
                        "Error"
                    ]
                }
            }
        },
        "cloudId": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "aadClientId": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "aadTenantId": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "reportedProperties": {
            "parameters": {
                "clusterName": {
                    "type": "string"
                },
                "clusterId": {
                    "type": "string"
                },
                "clusterVersion": {
                    "type": "string"
                },
                "lastUpdated": {}
            },
            "body": {
                "nodes": {
                    "$ref": "#/$keywords/nodes"
                }
            }
        },
        "nodes": {
            "parameters": {
                "name": {
                    "type": "string"
                },
                "id": {},
                "manufacturer": {
                    "type": "string"
                },
                "model": {
                    "type": "string"
                },
                "osName": {
                    "type": "string"
                },
                "osVersion": {
                    "type": "string"
                },
                "serialNumber": {
                    "type": "string"
                },
                "coreCount": {},
                "memoryInGiB": {}
            },
            "array": true
        },
        "trialDaysRemaining": {
            "propertyParameters": {
                "value": {}
            }
        },
        "billingModel": {
            "propertyParameters": {
                "value": {
                    "type": "string"
                }
            }
        },
        "registrationTimestamp": {
            "propertyParameters": {
                "value": {}
            }
        },
        "lastSyncTimestamp": {
            "propertyParameters": {
                "value": {}
            }
        },
        "lastBillingTimestamp": {
            "propertyParameters": {
                "value": {}
            }
        }
    },
    "$resources": {
        "clusters": {
            "provisioningState": {
                "$ref": "#/$keywords/provisioningState"
